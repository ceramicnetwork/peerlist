{
    "name": "ceramic-peerlist-tests",
    "version": "0.0.1",
    "author": "@v-stickykeys",
    "license": "(Apache-2.0 OR MIT)",
    "keywords": [
        "ceramic",
        "peerlist",
        "ipfs",
        "eth",
        "3box"
    ],
    "scripts": {
        "build": "tsc",
        "test": "./node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit",
        "test:dev": "JEST_CERAMIC_NETWORK=dev-unstable ./node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit",
        "test:clay": "JEST_CERAMIC_NETWORK=testnet-clay ./node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit",
        "test:fire": "JEST_CERAMIC_NETWORK=mainnet ./node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit"
    },
    "devDependencies": {
        "@ceramicnetwork/ipfs-daemon": "^1.0.7",
        "@ceramicnetwork/ipfs-topology": "1.1.7-rc.2",
        "@types/jest": "^26.0.24",
        "jest": "^26.6.3",
        "ts-jest": "^26.4.4",
        "typescript": "^4.2.4",
        "jest-resolver-enhanced": "^1.0.0"
    },
    "dependencies": {}
}